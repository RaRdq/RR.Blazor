<?xml version="1.0" encoding="utf-8"?>
<Project>
  <!-- RR.Blazor Auto-Configuration Props -->
  <!-- This file is automatically imported by NuGet when RR.Blazor is referenced -->
  
  <PropertyGroup>
    <!-- Enable RR.Blazor features by default -->
    <RRBlazorEnabled>true</RRBlazorEnabled>
    
  </PropertyGroup>
  
  <!-- Define constants for conditional compilation -->
  <PropertyGroup>
    <DefineConstants>$(DefineConstants);RRBLAZOR</DefineConstants>
  </PropertyGroup>
</Project>